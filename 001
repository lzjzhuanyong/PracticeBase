#include<iostream>
using namespace std;

int check(int key,int a[],int num){
	for(int i=0;i<num;i++){
		if(a[i]==key)
			return i;
	}
	return -1;
}

int main(){
    int arr[101]={0};
	int num = 0;
	int key = 0;
	cin>>num;
	for(int i=0;i<num;i++){
		cin>>arr[i];
	}
	
	cin>>key;
	
	int result = check(key,arr,num);
    
    cout<<result;
	
}
